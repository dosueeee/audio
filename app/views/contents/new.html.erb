<div class="form-container">
  <%= form_for(@content, url: "/contents/create") do |f| %>
      <%= f.label :audio_title %>
      <%= f.text_field :audio_title %>
      <%= f.label :media_name %>
      <%= f.text_field :media_name %>
      <%= f.label :media_url %>
      <%= f.url_field :media_url, size: "60"%>
      <%= f.label :audio_image %>
      <%= f.text_field :audio_image %>
      <%= f.label :audio_file %>
      <%= f.text_field :audio_file %>
      <%= f.label :audio_type %>
      <%= f.text_field :audio_type %>
      <div class="ui one bottom attached">
        <%= f.submit "登録", class: "ui button" %>
      </div>
  <% end %>
</div>